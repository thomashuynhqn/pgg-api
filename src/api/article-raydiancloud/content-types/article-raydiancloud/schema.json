{
  "kind": "collectionType",
  "collectionName": "article_raydianclouds",
  "info": {
    "singularName": "article-raydiancloud",
    "pluralName": "article-raydianclouds",
    "displayName": "ArticleRaydianCloud",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "maxLength": 300
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::article.article",
      "inversedBy": "article_raydianclouds"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "content": {
      "type": "richtext",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "og": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "raydiancloud_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag-raydiancloud.tag-raydiancloud",
      "inversedBy": "articles"
    },
    "publishDate": {
      "type": "datetime",
      "displayName": "Scheduled Publication",
      "description": "Set when this RaydianCloud article should be automatically published",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
